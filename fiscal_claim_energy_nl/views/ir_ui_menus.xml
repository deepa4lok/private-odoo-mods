<?xml version="1.0"?>
<openerp>
    <data>

        <menuitem name="Claims"
                  id="menu_claims"
                  groups="fiscal_claim_energy_nl.group_module_fiscal_claim_user"
                  />
        <menuitem name="Claims"
                  id="menu_claims_header"
                  parent="menu_claims"
                  sequence="10"
                  />
        <menuitem action="action_claims_view"
                  name="Claims"
                  id="menu_action_claims"
                  parent="menu_claims_header"
                  sequence="2"
                  groups=""
                  />

        <record id="action_claims_view" model="ir.actions.act_window">
            <field name="name">Debtors Claims</field>
            <field name="res_model">claim</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field eval="False" name="view_id"/>
            <field name="search_view_id" ref="view_claims_filter"/>
        </record>

        <record id="action_claim_tree_view" model="ir.actions.act_window.view">
            <field eval="1" name="sequence"/>
            <field name="view_mode">tree</field>
            <field name="act_window_id" ref="action_claims_view"/>
        </record>

        <record id="action_claim_tree_view1" model="ir.actions.act_window.view">
            <field eval="2" name="sequence"/>
            <field name="view_mode">form</field>
            <field name="view_id" ref="claims_form"/>
            <field name="act_window_id" ref="action_claims_view"/>
        </record>

        <!--Payment Line -->

        <record id="action_payment_line_view" model="ir.actions.act_window">
            <field name="name">Payment Lines</field>
            <field name="res_model">payment.line</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="search_view_id" ref="view_payment_line_filter"/>
        </record>

        <record id="action_payment_line_tree_view" model="ir.actions.act_window.view">
            <field eval="1" name="sequence"/>
            <field name="view_mode">tree</field>
            <field eval="payment_line_tree_view" name="view_id"/>
            <field name="act_window_id" ref="action_payment_line_view"/>
        </record>

        <record id="action_payment_line_form_view" model="ir.actions.act_window.view">
            <field eval="2" name="sequence"/>
            <field name="view_mode">form</field>
            <!--field name="view_id" eval="False"/-->
            <field name="act_window_id" ref="action_payment_line_view"/>
        </record>

        <menuitem action="action_payment_line_view"
                  name="Payment Lines"
                  id="menu_action_payment_line"
                  parent="menu_claims_header"
                  sequence="3"
                  groups=""
                  />

        <record id="action_cost_line_view" model="ir.actions.act_window">
            <field name="name">Cost Lines</field>
            <field name="res_model">cost.line</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="search_view_id" ref="view_cost_line_filter"/>
        </record>

        <record id="action_cost_line_tree_view" model="ir.actions.act_window.view">
            <field eval="1" name="sequence"/>
            <field name="view_mode">tree</field>
            <field eval="cost_line_tree_view" name="view_id"/>
            <field name="act_window_id" ref="action_cost_line_view"/>
        </record>

        <record id="action_cost_line_form_view" model="ir.actions.act_window.view">
            <field eval="2" name="sequence"/>
            <field name="view_mode">form</field>
            <!--field name="view_id" eval="False"/-->
            <field name="act_window_id" ref="action_cost_line_view"/>
        </record>

        <menuitem action="action_cost_line_view"
                  name="Cost Lines"
                  id="menu_action_cost_line"
                  parent="menu_claims_header"
                  sequence="4"
                  groups=""
                  />



    </data>
</openerp>